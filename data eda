mosaicplot(~교육+현재흡연, data =dat2, color=TRUE,    main ="19~20년도 교육 VS 현재흡연여부")
mosaicplot(~교육+현재흡연자금연의지, data =dat2, color=TRUE, main ="19~20년도 교육 VS 현재흡연자금연의지")

par(mfrow=c(2,2))
mosaicplot(~age+성별, data = dat2, color=TRUE, main ="19~20년도 age VS 성별")

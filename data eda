text(x=a,y=table(dat4$녹차빈도범주),labels=table(dat4$녹차빈도범주),pos=3,col="black")


a<-barplot(table(dat4$탄산빈도범주),main="탄산섭취빈도당  해당인원 수",ylim=c(0,2300))
